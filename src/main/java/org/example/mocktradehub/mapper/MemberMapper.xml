<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mocktradehub.mapper.MemberMapper">
    <select id="getUserById" resultType="Member" parameterType="String">
        SELECT *
        FROM MEMBERS
        WHERE member_id = #{member_id}
    </select>
</mapper>
